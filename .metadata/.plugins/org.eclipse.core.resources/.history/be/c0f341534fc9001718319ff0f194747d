public class Inizializing extends JWindow{
	
	private Timer timer;
	
	public Inizializing() {

		getContentPane().setBackground(Color.YELLOW);
		JTextField iniz = new JTextField("inizializing");
		add(iniz);
		timer = new Timer(20, (e)->{
			
		});

		
		setSize(200, 200);
		setVisible(true);
	}

	public static void main(String[] args) {

		new Inizializing();
	}

}
